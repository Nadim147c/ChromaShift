{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z_][a-zA-Z0-9_]*$": {
      "type": "object",
      "properties": {
        "regexp": {
          "type": "string"
        },
        "colors": {
          "type": "string"
        }
      },
      "required": ["regexp", "colors"]
    }
  }
}
