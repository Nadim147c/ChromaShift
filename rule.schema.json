{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": { "$schema": { "type": "string" } },
  "patternProperties": {
    "^[a-zA-Z_][a-zA-Z0-9_]*$": {
      "type": "object",
      "properties": {
        "regexp": {
          "type": "string"
        },
        "colors": {
          "type": "string"
        },
        "overwrite": {
          "type": "boolean",
          "default": "true"
        },
        "priority": {
          "type": "integer",
          "default": 0
        }
      },
      "required": ["regexp", "colors"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
